{
  "name": "nuxt-blank",
  "version": "1.0.0",
  "description": "nuxt-blank",
  "author": "Sergei Kornienko",
  "private": true,
  "scripts": {
    "svg-sprite": "svg-sprite --symbol --symbol-dest=static/images --symbol-sprite=sprite.svg assets/images/svg/*.svg",
    "prepeare": "yarn svg-sprite",
    "predev": "yarn prepeare",
    "prebuild": "yarn prepeare",
    "pregenerate": "yarn prepeare",
    "start": "nuxt start",
    "dev": "nuxt",
    "debug": "HOST=0.0.0.0 node --inspect node_modules/.bin/nuxt",
    "prod": "yarn build && nuxt start",
    "build": "nuxt build",
    "generate": "nuxt generate --fail-on-error",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "report": "nuxt build --analyze",
    "report-modern": "nuxt build --analyze --modern=client"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.3",
    "@nuxtjs/sitemap": "^0.2.0",
    "current-device": "^0.7.8",
    "detect-browser": "^4.0.4",
    "intersection-observer": "^0.5.1",
    "lodash.debounce": "^4.0.8",
    "lodash.throttle": "^4.1.1",
    "nuxt": "^2.6.2",
    "nuxt-i18n": "^5.8.2",
    "object.entries": "^1.1.1",
    "object.fromentries": "^2.0.2",
    "stylus-mixins": "https://github.com/qluqua/stylus-mixins.git",
    "svgxuse": "^1.2.6",
    "ts-node": "^8.1.0",
    "validator": "^10.8.0"
  },
  "devDependencies": {
    "@nuxt/vue-app": "^2.4.3",
    "@nuxtjs/proxy": "^1.3.3",
    "@types/chai": "^4.1.6",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.21",
    "bemto.pug": "^2.1.0",
    "cross-env": "^5.2.0",
    "dotenv": "^6.1.0",
    "eslint": "^5.0.1",
    "eslint-config-prettier": "^3.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-vue": "^4.0.0",
    "nodemon": "^1.11.0",
    "path": "^0.12.7",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "svg-sprite": "^1.5.0",
    "ts-loader": "^5.2.2",
    "typescript": "^3.1.3"
  }
}
